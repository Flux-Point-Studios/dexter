{
  "name": "@fluxpointstudios/dexter",
  "version": "0.0.19",
  "license": "MIT",
  "author": {
    "name": "Clark Alesna",
    "email": "clark@saib.dev"
  },
  "contributors": [
    {
      "name": "Zachary Sluder"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dexter",
    "Cardano",
    "DEX"
  ],
  "description": "Customizable Typescript SDK for interacting with Cardano DEXs (FluxPoint fork with SaturnSwap REST)",
  "repository": "https://github.com/Flux-Point-Studios/dexter",
  "homepage": "https://github.com/Flux-Point-Studios/dexter#readme",
  "bugs": {
    "url": "https://github.com/Flux-Point-Studios/dexter/issues"
  },
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "packageManager": "bun@1.2.22",
  "scripts": {
    "build": "bunx --bun tsc --project tsconfig.json && bunx --bun tsc-alias -p tsconfig.json",
    "test": "bun test",
    "prepare": "bun run build",
    "prepublishOnly": "bun test",
    "smoke:saturn": "bun scripts/saturn-amm-cli.mjs",
    "smoke:dex": "bun scripts/smoke-dex-apis.mjs",
    "smoke:dex:verbose": "bun scripts/smoke-dex-apis.mjs --verbose",
    "smoke:integration": "bun scripts/smoke-dexter-integration.mjs",
    "smoke": "bun run smoke:dex && bun run smoke:integration"
  },
  "dependencies": {
    "@lucid-evolution/lucid": "^0.4.29",
    "@splashprotocol/api": "2.0.0-beta.59",
    "@splashprotocol/cml-builder": "2.0.0-beta.133",
    "@splashprotocol/core": "2.0.0-beta.19",
    "@splashprotocol/sdk": "2.0.0-beta.132",
    "@types/blake2b": "^2.1.3",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "blake2b": "^2.1.4",
    "bottleneck": "^2.19.5",
    "crypto-js": "^4.2.0",
    "int64-buffer": "^1.1.0",
    "js-encoding-utils": "^0.7.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  }
}
